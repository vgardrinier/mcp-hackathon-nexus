# API Key from dashboard (get from /dashboard/account after signing up)
API_KEY=your-api-key-from-dashboard

# Dashboard URL (where your Next.js dashboard is running)
DASHBOARD_URL=http://localhost:3000

# HTTP Server Port (for MCP HTTP endpoint)
HTTP_SERVER_PORT=3001

# Optional: Allow unauthenticated MCP access (for testing only - NOT recommended in production)
# ALLOW_UNAUTHENTICATED_MCP=false
